<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox id="VBoxLogin" prefHeight="400.0" prefWidth="670.0" stylesheets="@../../css/main.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.mainControllers.LoginController">
   <children>
      <AnchorPane id="AnchorPane" fx:id="registerLabel" prefHeight="388.0" prefWidth="675.0">
         <children>
            <Button defaultButton="true" layoutX="259.0" layoutY="263.0" mnemonicParsing="false" onAction="#ActionLogin" prefHeight="35.0" prefWidth="85.0" text="%key.main.login" AnchorPane.bottomAnchor="90.0">
               <font>
                  <Font name="Sitka Small" size="12.0" />
               </font></Button>
            <TextField id="text-field" fx:id="emailField" layoutX="221.0" layoutY="149.0" prefHeight="45.0" prefWidth="245.0" promptText="%key.main.email" AnchorPane.leftAnchor="221.0" AnchorPane.rightAnchor="204.0" />
            <Label layoutX="269.0" layoutY="82.0" prefHeight="53.0" prefWidth="160.0" text="%key.main.authorization" textAlignment="CENTER" textFill="#5bc2d0" AnchorPane.bottomAnchor="253.0" AnchorPane.leftAnchor="269.0" AnchorPane.rightAnchor="241.0" AnchorPane.topAnchor="82.0">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <PasswordField fx:id="passwordField" layoutX="221.0" layoutY="206.0" prefHeight="45.0" prefWidth="255.0" promptText="%key.main.password" />
            <Button cancelButton="true" layoutY="2.0" mnemonicParsing="false" onAction="#ActionBack" prefHeight="25.0" prefWidth="60.0" text="%key.main.back">
               <font>
                  <Font name="Simplex" size="12.0" />
               </font>
            </Button>
            <Button layoutX="357.0" layoutY="263.0" mnemonicParsing="false" onAction="#ActionRegister" prefHeight="35.0" prefWidth="85.0" text="%key.main.register" />
         </children>
      </AnchorPane>
   </children>
</VBox>
